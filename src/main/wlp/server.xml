<server>

	<keyStore id="defaultKeyStore" password="keyStorePwd"/>

    <featureManager>
        <feature>webProfile-7.0</feature>
    </featureManager>

    <httpEndpoint id="defaultHttpEndpoint"
                  host="*"
                  httpPort="9080">
        <tcpOptions soReuseAddr="true"/>
    </httpEndpoint>

    <application name="WAT" context-root="/wat" location="${appLocation}" type="war" autoStart="false" id="WAT"/>

</server>
